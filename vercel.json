{
   "headers": [{
    "source": "/(.*)",
    "headers" : [
      {
        "key" : "Referer",
        "value" : "https://www.bing.com/new/"
      },
      {
        "key" : "Access-Control-Allow-Headers",
        "value" : "content-type"
      },
      {
        "key" : "Access-Control-Allow-Methods",
        "value" : "DELETE,PUT,POST,GET,OPTIONS"
      }
    ]
  }],
  "rewrites": [
    {"source": "/(.*)", "destination": "https://www.bing.com/new/$1"}
  ]
}
